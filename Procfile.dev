# Web Processes
advocato:     subcontract --chdir $DEV_ROOT_PATH/advocato --rbenv . -- rails s -p 3000 -b lvh.me
bossanova:    subcontract --chdir $DEV_ROOT_PATH/bossanova --rbenv . -- bundle exec rails s -p 4000 -b lvh.me
